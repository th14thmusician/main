<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@DarkTheme.css" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
   <GridPane alignment="CENTER" maxHeight="1.7976931348623157E308" prefHeight="809.0" prefWidth="367.0">
     <columnConstraints>
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="179.0" minWidth="10.0" prefWidth="110.0" />
       <ColumnConstraints hgrow="SOMETIMES" maxWidth="278.0" minWidth="10.0" prefWidth="257.0" />
     </columnConstraints>
     <rowConstraints>
         <RowConstraints maxHeight="56.0" minHeight="10.0" prefHeight="49.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="68.0" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="68.0" minHeight="10.0" prefHeight="61.0" vgrow="SOMETIMES" />
       <RowConstraints maxHeight="62.0" minHeight="10.0" prefHeight="56.0" vgrow="SOMETIMES" />
         <RowConstraints maxHeight="64.0" minHeight="10.0" prefHeight="64.0" vgrow="SOMETIMES" />
     </rowConstraints>
      <children>
            <Label fx:id="phone" styleClass="cell_small_label" text="\$phone" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label fx:id="address" prefHeight="29.0" prefWidth="257.0" styleClass="cell_small_label" text="\$address" GridPane.columnIndex="1" GridPane.rowIndex="3" />
          <Label fx:id="email" graphicTextGap="0.0" styleClass="cell_small_label" text="\$email" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         <Label fx:id="image" layoutX="120.0" layoutY="86.0" styleClass="cell_small_label" text="\$image" />
         <Label fx:id="phoneTitle" layoutX="10.0" layoutY="86.0" styleClass="cell_small_label" text="\$phone title" GridPane.rowIndex="2" />
         <Label fx:id="addressTitle" layoutX="10.0" layoutY="86.0" styleClass="cell_small_label" text="\\$addressTitle" GridPane.rowIndex="3" />
         <Label fx:id="emailTitle" layoutX="10.0" layoutY="30.0" styleClass="cell_small_label" text="\$emailTitle" GridPane.rowIndex="4" />
         <Label fx:id="groupTitle" layoutX="10.0" layoutY="286.0" styleClass="cell_small_label" text="\\$groupTitle" GridPane.rowIndex="5" />
         <Label fx:id="group" graphicTextGap="0.0" layoutX="120.0" layoutY="286.0" styleClass="cell_small_label" text="\$group" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" styleClass="cell_big_label" GridPane.columnIndex="1">
            <children>
                   <Label fx:id="name" styleClass="cell_small_label" stylesheets="@DarkTheme.css" text="\$first" />
               <FlowPane fx:id="tags" prefHeight="15.0" prefWidth="257.0" />
            </children>
         </VBox>
      </children>
   </GridPane>
</VBox>
